{
  "version": 2,
  "functions": {
    "api/gemini.js": {
      "runtime": "@vercel/node"
    },
    "api/rd-station.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    { "src": "/api/gemini", "dest": "/api/gemini.js" },
    { "src": "/api/rd-station", "dest": "/api/rd-station.js" },
    { "src": "/form.html", "dest": "/form.html" },
    { "src": "/images/(.*)", "dest": "/images/$1" },
    { "src": "/thumb/(.*)", "dest": "/thumb/$1" },
    { "src": "/(.*)\\.(mp4|png|svg|ico)", "dest": "/$1.$2" },
    { "src": "/(.*)\\.js", "dest": "/$1.js" },
    { "src": "/(.*)\\.css", "dest": "/$1.css" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
